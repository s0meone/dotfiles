<?xml version="1.0"?>
<root>
  <deviceproductdef>
    <productname>WASD_KEYBOARD</productname>
    <productid>0x04d9</productid>
  </deviceproductdef>
  <vkopenurldef>
    <name>KeyCode::VK_OPEN_URL_APP_SCREENSAVER</name>
    <url>file:///System/Library/Frameworks/ScreenSaver.framework/Versions/A/Resources/ScreenSaverEngine.app</url>
  </vkopenurldef>
  <item>
    <name>Daniel's settings</name>
    <appendix>Custom key mapping for a WASD Mechanical Keyboard.</appendix>
    <item>
      <name>Brightness Down to F1</name>
      <appendix>OSX's brightness key setting works best because it knows wich display to adjust.</appendix>
    </item>
    <item>
      <name>Brightness Up to F2</name>
      <appendix>OSX's brightness key setting works best because it knows wich display to adjust.</appendix>
    </item>
    <item>
      <name>Mission Control to F3</name>
      <identifier>private.daniel_mission_control</identifier>
      <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
      <autogen>__KeyToKey__ KeyCode::F3, KeyCode::MISSION_CONTROL</autogen>
    </item>
    <item>
      <name>Launchpad to F4</name>
      <identifier>private.daniel_launchpad</identifier>
      <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
      <autogen>__KeyToKey__ KeyCode::F4, ModifierFlag::NONE, KeyCode::LAUNCHPAD</autogen>
    </item>
    <item>
      <name>Lock Screen/Start Screensaver to F8</name>
      <identifier>private.daniel_lock_screen</identifier>
      <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
      <autogen>__KeyToKey__ KeyCode::F8, ModifierFlag::NONE, KeyCode::VK_OPEN_URL_APP_SCREENSAVER</autogen>
    </item>
    <item>
      <name>Music Prev to F9</name>
      <identifier>private.daniel_music_prev</identifier>
      <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
      <autogen>__KeyToConsumer__ KeyCode::F9, ModifierFlag::NONE, ConsumerKeyCode::MUSIC_PREV</autogen>
    </item>
    <item>
      <name>Music Play/Pause to F10</name>
      <identifier>private.daniel_music_play_pause</identifier>
      <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
      <autogen>__KeyToConsumer__ KeyCode::F10, ModifierFlag::NONE, ConsumerKeyCode::MUSIC_PLAY</autogen>
    </item>
    <item>
      <name>Music Next to F11</name>
      <identifier>private.daniel_music_next</identifier>
      <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
      <autogen>__KeyToConsumer__ KeyCode::F11, ModifierFlag::NONE, ConsumerKeyCode::MUSIC_NEXT</autogen>
    </item>
    <item>
      <name>Eject to F12</name>
      <identifier>private.daniel_music_eject</identifier>
      <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
      <autogen>__KeyToConsumer__ KeyCode::F12, ModifierFlag::NONE, ConsumerKeyCode::EJECT</autogen>
    </item>
    <item>
      <name>Volume Mute to F13</name>
      <identifier>private.daniel_volume_mute</identifier>
      <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
      <autogen>__KeyToConsumer__ KeyCode::F13, ConsumerKeyCode::VOLUME_MUTE</autogen>
    </item>
    <item>
      <name>Volume Down to F14</name>
      <identifier>private.daniel_volume_down</identifier>
      <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
      <autogen>__KeyToConsumer__ KeyCode::F14, ConsumerKeyCode::VOLUME_DOWN</autogen>
    </item>
    <item>
      <name>Volume Up to F15</name>
      <identifier>private.daniel_volume_up</identifier>
      <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
      <autogen>__KeyToConsumer__ KeyCode::F15, ConsumerKeyCode::VOLUME_UP</autogen>
    </item>
  </item>
</root>
